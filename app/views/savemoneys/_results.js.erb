<table>
	<tr>
	<th>Magasin</th>
	<th>Produit</th>
	<th>Format</th>
	<th>Prix</th>
	</tr>
  <% @results.each do |result| %>
  	<tr>
  	<td><%= result[0].magasin.nom %></td>
  	<td><%= result[0].produit.produit_nom.nom %></td>
  	<td><%= result[0].produit.format.nom %></td>
  	<td><%= result[0].prix_regulier %></td>
  	</tr>
  <% end %>
</table>